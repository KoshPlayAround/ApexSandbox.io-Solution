public Boolean ascendingOrder(Integer a, Integer b, Integer c) {
    /*if(a<b && b<c){
        return true;
    }else if(a == b && b<c && a != c){
       return true;
    }else if(a == b && b == c){
       return true;
    }else{
        return false;
    }*/

    return (a < b && b < c) || (a == b && b < c && a != c) || (a == b && b == c);

}