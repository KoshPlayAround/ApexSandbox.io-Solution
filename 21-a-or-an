public String aOrAn(String word) {
    if(word.s1.startsWithIgnoreCase('a') 
    || word.s1.startsWithIgnoreCase('e') 
    || word.s1.startsWithIgnoreCase('i')
    || word.s1.startsWithIgnoreCase('o')
    || word.s1.startsWithIgnoreCase('u')){
     return 'an '+word;
    }else{
        return 'a '+word;
    }

    
}